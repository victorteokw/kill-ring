{
	"name": "kill-ring",
	"displayName": "Kill Ring",
	"description": "Kill Ring for VSCode.",
	"publisher": "yeannylam",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.34.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/zhangkaiyulw/kill-ring.git"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:killRing.killLine",
		"onCommand:killRing.killRegion",
		"onCommand:killRing.killRingSave"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "killRing.killLine",
				"title": "Kill Line",
				"category": "Kill Ring"
			},
			{
				"command": "killRing.killRegion",
				"title": "Kill Region",
				"category": "Kill Ring"
			},
			{
				"command": "killRing.killRingSave",
				"title": "Kill Ring Save",
				"category": "Kill Ring"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}
